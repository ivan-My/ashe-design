{
  "name": "ashe-design3",
  "version": "1.0.0",
  "description": "",
  "main": "dist/ahse.react.umd.js",
  "module": "dist/esm/ashe-react.es.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "add": "node scripts/create-component.js",
    "dev": "npm run checked && vite --open --force",
    "generate:file": "node scripts/generate-ashe.js",
    "generate:themes": "node scripts/generate-themes.js",
    "checked": "npm run generate:file ",
    "build:loader-style": "node scripts/generate-loader-style.js",
    "build": "npm run checked && npm run build:vite && npm run build:es && npm run build:css && npm run build:loader-style && npm run build:dts",
    "build:vite": "vite build --config vite.config.build.ts",
    "build:es": "  NODE_ENV=production rollup -c rollup.config.es.js",
    "build:dts2": "tsc --project build/tsconfig/tsconfig.d.json && npx rollup -c rollup.config.dts.js",
    "build:dts": "tsc  && npx rollup -c rollup.config.dts.js",
    "build:css": "npm run generate:themes && vite build --config vite.config.build.css.ts",
    "prepare": "husky install",
    "commit": "git-cz && npm run changelog",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.19.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@commitlint/config-conventional": "^17.1.0",
    "@rollup/plugin-babel": "^6.0.2",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/node": "^18.11.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.12",
    "commitizen": "^4.2.5",
    "commitlint": "^17.1.2",
    "commitlint-config-cz": "^0.13.3",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^7.26.0",
