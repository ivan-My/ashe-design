    "vinyl-fs": "^3.0.3",
    "vite": "^3.1.8"
  },
  "dependencies": {
    "@babel/runtime": "^7.19.4",
    "classnames": "^2.3.2",
    "react-router-dom": "^6.4.2"
  },
  "lint-staged": {
    "*.md": "prettier --write",
    "*.{ts,tsx,js,scss}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
