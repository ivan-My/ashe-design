{
    "name": "ashe-design",
    "version": "4.0.8",
    "style": "dist/style.css",
    "main": "dist/ahse.react.umd.js",
    "module": "dist/esm/ashe-react.es.js",
    "typings": "dist/types/index.d.ts",
    "description": "轻量级移动端 React 组件库",
    "author": "ivan",
    "files": [
        "dist",
        "package.json",
        "CHANGELOG.md",
        "README.md"
    ],
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    },
    "scripts": {
        "add": "node scripts/create-component.js && pnpm run gen:file ",
        "rm": "node scripts/remove-component.js && pnpm run gen:file",
        "dev": "pnpm run checked && vite --open --force",
        "gen:file": "node scripts/gen-ashe.js",
        "gen:themes": "node scripts/gen-themes.js",
        "checked": "pnpm run gen:file &&  tsc",
        "build": "rimraf dist  && pnpm run checked && pnpm run build:vite && pnpm run build:es && pnpm run build:css && pnpm run build:loader-style && pnpm run build:component:css",
        "build:es": "vite build --config vite.config.build.disperse.ts",
        "build:vite": "vite build --config vite.config.build.ts",
        "build:css": "pnpm run gen:themes && vite build --config vite.config.build.css.ts",
        "build:loader-style": "node scripts/gen-loader-style.js",
        "build:component:css": "node scripts/build-component-css.js",
        "build:site": "pnpm run checked && vite build --config vite.config.build.site.ts",
        "prepare": "husky install",
        "commit": "git-cz ",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
        "test": "jest",
        "release": "release-it"
    },
    "keywords": [
        "react",
        "webpack",
        "react component",
        "vite"
    ],
    "license": "ISC",
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-object-rest-spread": "^7.19.4",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.19.6",
        "@babel/preset-env": "^7.19.4",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.18.6",
        "@commitlint/config-conventional": "^17.1.0",
        "@release-it/conventional-changelog": "^5.1.1",
        "@rollup/plugin-babel": "^6.0.2",
        "@rollup/plugin-commonjs": "^23.0.2",
        "@rollup/plugin-node-resolve": "13.1.3",
        "@rollup/plugin-typescript": "^9.0.2",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@types/jest": "^29.2.0",
        "@types/lodash-es": "^4.17.6",
        "@types/node": "^18.11.3",
        "@types/postcss-import": "^14.0.0",
        "@types/react": "^18.0.21",
        "@types/react-dom": "^18.0.6",
        "@types/react-syntax-highlighter": "^15.5.6",
        "@typescript-eslint/eslint-plugin": "^5.46.0",
        "@typescript-eslint/parser": "^5.46.0",
        "@vitejs/plugin-react": "^2.1.0",
        "@vitejs/plugin-react-refresh": "^1.3.6",
        "@vitejs/plugin-react-swc": "^3.0.1",
        "autoprefixer": "^10.4.12",
        "babel-jest": "^29.2.2",
        "commitizen": "4.2.4",
        "commitlint": "^17.1.2",
        "commitlint-config-cz": "^0.13.3",
        "conventional-changelog-cli": "^2.2.2",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^7.26.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.23.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-markdown": "^2.2.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.23.2",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-unused-imports": "^1.1.5",
        "fs-extra": "10.0.1",
        "glob": "^8.0.3",
        "husky": "^8.0.1",
        "inquirer": "^8.0.0",
        "jest": "^29.2.2",
        "jest-environment-jsdom": "^29.2.2",
        "lint-staged": "^13.0.3",
        "lodash-es": "^4.17.21",
        "postcss-import": "^15.0.0",
        "prettier": "^2.7.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-markdown": "^8.0.3",
        "react-syntax-highlighter": "^15.5.0",
        "release-it": "^15.11.0",
        "remark-directive": "^2.0.1",
        "remark-gfm": "^3.0.1",
        "rimraf": "^3.0.2",
        "rollup": "2.68.0",
        "sass": "^1.55.0",
        "ts-jest": "^29.0.3",
        "typescript": "^4.8.4",
        "vite": "^4.0.1",
        "vite-plugin-dts": "^2.3.0"
    },
    "dependencies": {
        "@babel/runtime": "^7.19.4",
        "@bem-react/classname": "^1.5.12",
        "async-validator": "^4.2.5",
        "classnames": "^2.3.2",
        "react-router-dom": "^6.4.2"
    },
    "repository": "https://github.com/ivan-My/ashe-design",
    "lint-staged": {
        "*.md": "prettier --write",
        "*.{ts,tsx,js,scss}": "prettier --write"
    },
    "peerDependencies": {
        "react": "^18.0.0"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
