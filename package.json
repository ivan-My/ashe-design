        "build": "npm run build:rollup && npm run build:dts && npm run build:css",
        "build:rollup": "rimraf dist && NODE_ENV=production rollup -c build/rollup/rollup.config.base.js",
        "build:dts": "tsc --project build/tsconfig/tsconfig.d.json && npx rollup -c rollup.config.dts.js",
        "build:css": " vite build --config vite.config.build.css.ts"
    },
